{
  "manifest_version": 2,
  "name": "WhatsDis",
  "version": "1.0",
  "description": "WhatsDis ext",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon48.png",
      "38": "img/icon48.png"
    },
    "default_title": "WhatsDis",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["index.js"]
    }
  ],
  "web_accessible_resources": ["js/*"]
}
